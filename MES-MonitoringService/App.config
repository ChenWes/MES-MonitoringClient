<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
    </startup>
  <appSettings>
    <!--上传数据服务基础设置*************************************************************-->
    <!--上传数据服务名称-->
    <add key="DefaultServiceName" value="MESUploadDataService"/>
    <!--上传数据服务显示名称-->
    <add key="DefaultServiceDisplayName" value="MES Upload Data Service"/>
    <!--上传数据服务描述-->
    <add key="DefaultServiceDescription" value="MES后台数据上传服务，请勿手动停止，如该服务停止，将无法正常向服务器上传数据。"/>


    <!--MongoDB设置*************************************************************-->    
    <!--Mongodb服务名称（基本固定，不需更改）-->
    <add key="MongodbServiceName" value="MongoDB"/>    
    <!--Mongodb本地服务器地址-->
    <add key="MongodbURL" value="mongodb://localhost:27017"/>    
    <!--Mongodb本地数据库名称-->
    <add key="MongodbName" value="MES"/>


    
    <!--RabbitMQ运行参数*************************************************************-->
    <!--RabbitMQ服务器地址-->
    <add key="RabbitMQServerHostName" value="localhost"/>    
    <!--上传数据隔多少秒（参数是毫秒）-->
    <add key="RabbitMQUserName" value="guest"/>    
    <!--上传数据隔多少秒（参数是毫秒）-->
    <add key="RabbitMQPassword" value="guest"/>
    
    

    <!--上传数据服务运行参数*************************************************************-->
    <!--上传数据隔多少秒（参数是毫秒）-->
    <add key="UploadDataIntervalMilliseconds" value="1000"/>
    
    
    
    <!--机器状态日志队列名称-->
    <add key="MachineStatusLogQueueName" value="MachineStatusLog"/>    
    <!--机器状态日志队列名称-->
    <add key="MachineStatusCollectionName" value="StatusLog"/>


    
    
  </appSettings>
</configuration>
