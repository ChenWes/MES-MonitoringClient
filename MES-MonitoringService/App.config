    <!--工单数据集合名称，即表名-->
    <add key="JobOrderFirstProduceLogCollectionName" value="JobOrderFirstProduceLog" />
    <!--车间负责人集合名-->
    <add key="WorkshopResponsiblePersonCollectionName" value="WorkshopResponsiblePerson"/>
    <!--机器负责人-->
    <add key="MachineResponsiblePersonCollectionName" value="MachineResponsiblePerson"/>
    <!--员工排班-->
    <add key="EmployeeWorkScheduleCollectionName" value="EmployeeWorkSchedule"  />
    <!--机器生产数-->
    <add key="MachineProductionCollectionName" value="MachineProduction"  />
    <!--后台服务器设置*************************************************************-->
    <add key="BackendServerProtocol" value="http" />
    <add key="BackendServerHost" value="localhost" />
    <add key="BackendServerPort" value="3000" />
